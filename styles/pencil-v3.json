{
  "version": 3,
  "sprite": "https://www.mapbox.com/mapbox-gl-styles/sprites/pencil",
  "constants": {
    "@land": "rgba(0,0,0,0)",
    "@water": "#73b6e6",
    "@park": "#c8df9f",
    "@road": "#fefefe",
    "@border": "#6d90ab",
    "@wood": "#33AA66",
    "@building": "#d9ccbe",
    "@building_outline": "#d2c6b9",
    "@text": "#000000",
    "@satellite_brightness_low": 0,
    "@satellite_brightness_high": 1,
    "@satellite_saturation": 2,
    "@satellite_spin": 0
  },
  "sources": {
    "mapbox.mapbox-streets-v5": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v5",
      "glyphs": "http://mapbox.s3.amazonaws.com/gl-glyphs-256/{fontstack}/{range}.pbf",
      "tileSize": 512
    }
  },
  "layers": [{
    "id": "background",
    "render": {
      "type": "background"
    },
    "style": {
      "background-color": "@land",
      "background-image": "natural_paper"
    }
  }, {
    "id": "park",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "landuse",
    "filter": { "class": "park" },
    "render": {
      "type": "fill"
    },
    "style": {
      "fill-color": "@park",
      "fill-image": "shade_medium",
      "fill-antialias": true
    }
  }, {
    "id": "park_outline",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "landuse",
    "filter": { "class": "park", "$type": "polygon" },
    "render": {
      "type": "line"
    },
    "style": {
      "line-image": "line_shade_22_1",
      "line-opacity": 0.7,
      "line-color": "#eeeeee",
      "line-width": {
        "fn": "exponential",
        "z": 8,
        "val": 1,
        "slope": 0.2,
        "min": 1,
        "max": 6
      }
    }
  }, {
    "id": "wood",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "landuse",
    "filter": { "class": "wood" },
    "render": {
      "type": "fill"
    },
    "style": {
      "fill-color": "@wood",
      "fill-opacity": 0.08,
      "fill-antialias": true
    }
  }, {
    "id": "waterway",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "waterway",
    "render": {
      "type": "line"
    },
    "style": {
      "line-color": "@water",
      "line-width": {
        "fn": "linear",
        "z": 8,
        "val": 1,
        "slope": 0.5,
        "min": 0.5
      }
    }
  }, {
    "id": "water",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "water",
    "render": {
      "type": "fill"
    },
    "style": {
      "fill-color": "@water",
      "fill-image": "shade_medium",
      "fill-antialias": true
    }
  }, {
    "id": "roads",
    "layers": [{
      "id": "road_large_casing",
      "source": "mapbox.mapbox-streets-v5",
      "source-layer": "road",
      "filter": { "class": ["motorway", "main"] },
      "render": {
        "type": "line",
        "line-cap": "butt",
        "line-join": "bevel"
      },
      "style": {
        "line-color": "rgba(152,152,152,1)",
        "line-width": 0,
        "line-opacity": {
          "fn": "linear",
          "z": 14.5,
          "val": 0,
          "slope": 1,
          "min": 0,
          "max": 1
        }
      }
    }, {
      "id": "road_regular_casing",
      "source": "mapbox.mapbox-streets-v5",
      "source-layer": "road",
      "filter": { "class": ["street"] },
      "render": {
        "type": "line",
        "line-cap": "butt",
        "line-join": "bevel"
      },
      "style": {
        "line-color": "rgba(152,152,152,1)",
        "line-opacity": {
          "fn": "linear",
          "z": 14.5,
          "val": 0,
          "slope": 1,
          "min": 0,
          "max": 1
        },
        "line-width": {
          "fn": "exponential",
          "z": 9,
          "val": -1,
          "slope": 0.2,
          "min": 1,
          "max": 7
        }
      }
    }, {
      "id": "road_limited",
      "source": "mapbox.mapbox-streets-v5",
      "source-layer": "road",
      "filter": { "class": ["street_limited"] },
      "render": {
        "type": "line",
        "line-cap": "round",
        "line-join": "bevel",
        "line-round-limit": 0.7
      },
      "style": {
        "line-dasharray": [10, 2],
        "line-color": "@road",
        "line-image": "line_solid_7",
        "line-opacity": 0.7,
        "line-width": {
          "fn": "exponential",
          "z": 9,
          "val": -1,
          "slope": 0.2,
          "min": 1,
          "max": 7
        }
      }
    }, {
      "id": "road_large",
      "ref": "road_large_casing",
      "style": {
        "line-color": "@road",
        "line-image": "line_double_20",
        "line-width": {
          "fn": "exponential",
          "z": 8,
          "val": -1,
          "slope": 0.2,
          "min": 1,
          "max": 20
        }
      }
    }, {
      "id": "road_regular",
      "ref": "road_regular_casing",
      "style": {
        "line-image": "line_double_18",
        "line-color": "@road",
        "line-width": {
          "fn": "exponential",
          "z": 9,
          "val": -3,
          "slope": 0.2,
          "min": 1,
          "max": 10
        }
      }
    }, {
      "id": "path",
      "source": "mapbox.mapbox-streets-v5",
      "source-layer": "road",
      "filter": { "class": ["path"] },
      "render": {
        "type": "line",
        "line-cap": "round",
        "line-join": "bevel"
      },
      "style": {
        "line-color": "rgba(255,255,255,1)",
        "line-dasharray": [2, 2],
        "line-image": "line_dotted_8",
        "line-opacity": 0.7,
        "line-width": 2
      }
    }, {
      "id": "bridge_large_casing",
      "source": "mapbox.mapbox-streets-v5",
      "source-layer": "bridge",
      "filter": { "class": ["motorway", "main"] },
      "render": {
        "type": "line"
      },
      "style": {
        "line-color": "rgba(152,152,152,1)",
        "line-width": 0,
        "line-opacity": {
          "fn": "linear",
          "z": 13,
          "val": 0,
          "slope": 1,
          "min": 0,
          "max": 1
        }
      }
    }, {
      "id": "bridge_large",
      "ref": "bridge_large_casing",
      "style": {
        "line-color": "@road",
        "line-image": "line_double_20",
        "line-width": {
          "fn": "exponential",
          "z": 8,
          "val": -1,
          "slope": 0.2,
          "min": 1,
          "max": 20
        }
      }
    }, {
      "id": "bridge_regular_casing",
      "source": "mapbox.mapbox-streets-v5",
      "source-layer": "bridge",
      "filter": { "class": ["street", "street_limited"] },
      "render": {
        "type": "line"
      },
      "style": {
        "line-color": "rgba(152,152,152,1)",
        "line-opacity": {
          "fn": "linear",
          "z": 14.5,
          "val": 0,
          "slope": 1,
          "min": 0,
          "max": 1
        },
        "line-width": {
          "fn": "exponential",
          "z": 9,
          "val": -1,
          "slope": 0.2,
          "min": 1,
          "max": 7
        }
      }
    }, {
      "id": "bridge_regular",
      "ref": "bridge_regular_casing",
      "style": {
        "line-image": "line_double_18",
        "line-color": "@road",
        "line-width": {
          "fn": "exponential",
          "z": 9,
          "val": -3,
          "slope": 0.2,
          "min": 1,
          "max": 10
        }
      }
    }],
    "style": {
      "composite-opacity": 1
    },
    "render": {
      "type": "composite"
    }
  }, {
    "id": "building",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "building",
    "render": {
      "type": "fill"
    },
    "style": {
      "fill-color": "@building",
      "fill-antialias": true,
      "fill-image": "shade_medium",
      "fill-opacity": {
        "fn": "linear",
        "z": 13,
        "val": 0,
        "slope": 1,
        "min": 0,
        "max": 1
      }
    }
  }, {
    "id": "building_outline",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "building",
    "filter": { "$type": "polygon" },
    "render": {
      "type": "line",
      "line-cap": "butt",
      "line-join": "bevel"
    },
    "style": {
      "line-width": {
        "fn": "exponential",
        "z": 12,
        "val": -1,
        "slope": 0.2,
        "min": 0,
        "max": 3
      },
      "line-color": "@land",
      "line-opacity": 0.5,
      "line-image": "line_solid_7"
    }
  }, {
    "id": "borders",
    "source": "mapbox.mapbox-streets-v5",
    "source-layer": "admin",
    "render": {
      "type": "line"
    },
    "style": {
      "line-color": "rgba(0,0,0,0.3)",
      "line-width": 1
    }
  }]
}
